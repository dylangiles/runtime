{
  "enabled": true,
  "extends": ["config:base"],
  "enabledManagers": [
    "bazel",
    "cloudbuild",
    "dockerfile",
    "github-actions",
    "git-submodules",
    "npm",
    "bazelisk"
  ],
  "baseBranches": ["main"],
  "pinDigests": true,
  "rangeStrategy": "pin",
  "updateLockFiles": true,
  "dependencyDashboard": true,
  "dependencyDashboardApproval": true,
  "lockFileMaintenance": { "enabled": true },
  "labels": ["dependencies"]
}
